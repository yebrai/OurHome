<div>
    <h2>List of all professionals</h2>
</div>

<div class="background-list-professional">
  <div class="container mx-auto mt-4 ">
    <div class="row">
    {{#each professionalList}}
      <div class="col-md-4">
        <div class="card" style="width: 18rem;">
            <img src="{{this.img}}" class="card-img-top" alt="img-property" />
          <div class="card-body">
            <h5 class="card-title">Name: {{name}}.</h5>
            <p class="card-text">Cif: {{cif}}.</p>
            <p class="card-text">E-mail: {{email}}</p>
            <p class="card-text">Phone: {{phone}}.</p>
          </div>
        </div>
      </div>    
    {{/each}}
    </div>
  </div>
</div>
